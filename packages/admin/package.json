{
    "name": "cloudbase-cms",
    "version": "2.0.0",
    "private": true,
    "description": "An out-of-box UI solution for enterprise applications",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "deploy": "npm run site && npm run gh-pages",
        "dev": "npm run start:dev",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "postinstall": "umi g tmp",
        "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_UI=none umi dev",
        "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
        "tsc": "tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/icons": "^4.0.0",
        "@ant-design/pro-card": "^1.0.0-beta.2",
        "@ant-design/pro-layout": "^6.0.0",
        "@ant-design/pro-table": "^2.4.0",
        "@umijs/hooks": "^1.9.3",
        "antd": "^4.4.0",
        "classnames": "^2.2.6",
        "concent": "^2.7.27",
        "css.gg": "^2.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.25.3",
        "omit.js": "^2.0.2",
        "qs": "^6.9.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet-async": "^1.0.4",
        "umi": "^3.1.2",
        "umi-request": "^1.0.8",
        "use-merge-value": "^1.0.1"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^2.0.2",
        "@types/classnames": "^2.2.7",
        "@types/express": "^4.17.0",
        "@types/history": "^4.7.2",
        "@types/lodash": "^4.14.144",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.8.4",
        "@types/react-helmet": "^5.0.13",
        "@umijs/plugin-blocks": "^2.0.5",
        "@umijs/plugin-esbuild": "^1.0.0-beta.2",
        "@umijs/preset-ant-design-pro": "^1.2.2",
        "@umijs/preset-react": "^1.5.19",
        "@umijs/preset-ui": "^2.1.11",
        "concent-plugin-redux-devtool": "^2.0.2",
        "cross-env": "^7.0.0",
        "cross-port-killer": "^1.1.1",
        "detect-installer": "^1.0.1",
        "express": "^4.17.1",
        "mockjs": "^1.0.1-beta3",
        "pro-download": "1.0.1"
    },
    "engines": {
        "node": ">=10.0.0"
    }
}
